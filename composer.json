{
    "name": "ajaxray/magic",
    "description": "Simple Auto-wiring, PSR-11 compliant Dependency injection library for PHP 8.",
    "keywords": ["dependency", "dependency-injection-container", "php-di", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anis Uddin Ahmad",
            "email": "anis.programmer@gmail.com"
        }
    ],
    "type": "library",
    "require": {
		"php": ">=8.0",
        "psr/container": "1.1.1"
    },
    "require-dev": {
		"phpunit/phpunit": "9.*",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ajaxray\\Magic\\": "src/",
            "Ajaxray\\Test\\": "tests/"
        }
    }
}
